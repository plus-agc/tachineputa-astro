    order: 1;
  }

  .c-card__thumbnail {
    order: 0;
  }

  .c-card__image {
    width: 100%;
  }

  .roll {
    transition-duration: 1s;
    transition-property: opacity, transform, letter-spacing;
    transition-timing-function: cubic-bezier(0.165, 0.84, 0.44, 1);
    will-change: transform;
  }

  @media screen and (min-width: 768px) {
    .roll[data-tabdelay="0.1s"] {
      transition-delay: 0.1s;
    }

    .roll[data-tabdelay="0.2s"] {
      transition-delay: 0.2s;
    }

    .roll[data-tabdelay="0.3s"] {
      transition-delay: 0.3s;
    }

    .roll[data-tabdelay="0.4s"] {
      transition-delay: 0.4s;
    }
  }

  @media screen and (min-width: 768px) {
    .roll[data-tabgroup="2"]:nth-child(odd) {
      transition-delay: 0s;
    }

    .roll[data-tabgroup="2"]:nth-child(2n + 2) {
      transition-delay: 0.1s;
    }

    .roll[data-tabgroup="3"]:nth-child(3n + 1) {
      transition-delay: 0s;
    }

    .roll[data-tabgroup="3"]:nth-child(3n + 2) {
      transition-delay: 0.1s;
    }

    .roll[data-tabgroup="3"]:nth-child(3n + 3) {
      transition-delay: 0.2s;
    }

    .roll[data-tabgroup="4"]:nth-child(4n + 1) {
      transition-delay: 0s;
    }

    .roll[data-tabgroup="4"]:nth-child(4n + 2) {
      transition-delay: 0.1s;
    }

    .roll[data-tabgroup="4"]:nth-child(4n + 3) {
      transition-delay: 0.2s;
    }

    .roll[data-tabgroup="4"]:nth-child(4n + 4) {
      transition-delay: 0.3s;
    }
  }

  @media screen and (min-width: 1024px) {
    .roll[data-lapgroup="4"]:nth-child(4n + 1) {
      transition-delay: 0s;
    }

    .roll[data-lapgroup="4"]:nth-child(4n + 2) {
      transition-delay: 0.1s;
    }

    .roll[data-lapgroup="4"]:nth-child(4n + 3) {
      transition-delay: 0.2s;
    }

    .roll[data-lapgroup="4"]:nth-child(4n + 4) {
      transition-delay: 0.3s;
    }
  }

  .roll[data-spdelay="0.1s"] {
    transition-delay: 0.1s;
  }

  .roll[data-spdelay="0.2s"] {
    transition-delay: 0.2s;
  }

  .roll[data-spdelay="0.3s"] {
    transition-delay: 0.3s;
  }

  .roll[data-spdelay="0.4s"] {
    transition-delay: 0.4s;
  }

  .roll[data-spgroup="2"]:nth-child(odd) {
    transition-delay: 0s;
  }

  .roll[data-spgroup="2"]:nth-child(2n + 2) {
    transition-delay: 0.1s;
  }

  .roll[data-spgroup="3"]:nth-child(3n + 1) {
    transition-delay: 0s;
  }

  .roll[data-spgroup="3"]:nth-child(3n + 2) {
    transition-delay: 0.1s;
  }

  .roll[data-spgroup="3"]:nth-child(3n + 3) {
    transition-delay: 0.2s;
  }

  .roll[data-duration="fast"] {
    transition-duration: 0.3s;
  }

  .roll[data-duration="normal"],
  .roll[data-duration="slow"] {
    transition-duration: 1s;
  }

  .roll[data-type~="fadeIn"].is-rollReady {
    opacity: 0;
  }

  .roll[data-type~="fadeIn"].is-rollActive {
    opacity: 1;
  }

  .roll[data-type~="slideUpIn"].is-rollReady {
    transform: translateY(2.5rem);
    opacity: 0;
  }

  .roll[data-type~="slideUpIn"].is-rollActive {
    transform: translateY(0);
